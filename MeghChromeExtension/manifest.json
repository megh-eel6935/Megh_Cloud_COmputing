{
  "manifest_version": 2,
  "name": "Megh",
  "description": "This extension transfers data across devices",
  "version": "1.0",
 "background": 
    {
        "scripts": ["jquery-2.1.1.min.js","main.js"]
    },
  "permissions": [
    "contextMenus",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
